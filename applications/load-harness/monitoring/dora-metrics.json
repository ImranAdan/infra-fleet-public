          "showPercentChange": false,
          "textMode": "value",
          "wideLayout": true
        },
        "pluginVersion": "11.4.0",
        "targets": [
          {
            "datasource": { "type": "prometheus", "uid": "prometheus" },
            "expr": "count(dora_workflow_deploy_timestamp{status=\"success\"} > time() - 86400)",
            "refId": "A"
          }
        ],
        "title": "Workflow Deploys (24h)",
        "type": "stat"
      },
      {
        "datasource": { "type": "prometheus", "uid": "prometheus" },
