        "options": {
          "showHeader": true
        },
        "pluginVersion": "11.4.0",
        "targets": [
          {
            "datasource": { "type": "prometheus", "uid": "prometheus" },
            "expr": "100 * count by (source) (dora_workflow_deploy_timestamp{status=\"failure\"} > time() - 86400) / clamp_min(count by (source) (dora_workflow_deploy_timestamp > time() - 86400), 1)",
            "refId": "A"
          }
        ],
        "title": "Workflow Failure Rate (24h)",
        "type": "table"
